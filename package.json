{
  "name": "qoi-viewer",
  "displayName": "QOI Viewer",
  "description": "QOI Viewer for Visual Studio Code",
  "publisher": "Mupceet",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mupceet/qoi-viewer"
  },
  "version": "0.1.0",
  "icon": "images/qoi_logo.png",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCustomEditor:qoi.previewEditor"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "qoi.previewEditor",
        "displayName": "%customEditors.displayName%",
        "priority": "default",
        "selector": [
          {
            "filenamePattern": "*.qoi"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "qoi.zoomIn",
        "title": "%command.zoomIn%",
        "category": "Image Preview"
      },
      {
        "command": "qoi.zoomOut",
        "title": "%command.zoomOut%",
        "category": "Image Preview"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "qoi.zoomIn",
          "when": "activeCustomEditorId == 'qoi.previewEditor'",
          "group": "1_qoi"
        },
        {
          "command": "qoi.zoomOut",
          "when": "activeCustomEditorId == 'qoi.previewEditor'",
          "group": "1_qoi"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/pngjs": "^6.0.1",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "pngjs": "^6.0.0",
    "qoi-img": "^2.1.0",
    "validate-color": "^2.2.1",
    "vscode-nls": "^5.2.0"
  }
}
